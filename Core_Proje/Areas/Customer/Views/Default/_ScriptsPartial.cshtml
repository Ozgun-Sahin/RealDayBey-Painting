<script>
    $(function () {

        $("body").on("click", ".btn", function () {

            var url = $(this).data("target");

            $.get(url, function (data) { })

                .done(function (data) {

                    $("#modelView .modal-body").html(data);

                    $("#modelView").modal("show");
                })

                .fail(function () {

                    $("#modelView .modal-body").text("Error!!");

                    $("#modelView").modal("show");
                })

        });
    })
</script>